{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "ForwardProxies": {
        "Enabled": "true",
        "BasePath": "/Pharmanet",
        "KnownProxies": []
    },
    "AllowedHosts": "*",
    "SwaggerSettings": {
        "RoutePrefix": "swagger",
        "Info": {
            "Title": "Health.PharmaNet MedicationRequest API documentation",
            "Description": "Provides API documentation for the eRx API MedicationRequest service.",
            "License": {
                "Name": "Apache 2.0",
                "Url": "https://www.apache.org/licenses/LICENSE-2.0.html"
            }
        }
    },
    "Hl7v2Authorization": {
        "MessageTypeKeys": {
            "ZPN": {
                "KeyTemplate": "MSH.9_ZZZ.1_ZCA.3"
            }
        },
        "MessageScopes": {
            "ZPN_TRX_X0": {
                "Scope": "system/MedicationRequest.read system/*.read"
            },
            "ZPN_TRX_X1": {
                "Scope": "system/MedicationRequest.write system/*.write"
            },
            "ZPN_TRX_X2": {
                "Scope": "system/MedicationRequest.write system/*.write"
            },
            "ZPN_TRX_X3": {
                "Scope": "system/MedicationRequest.write system/*.write"
            },
            "ZPN_TRX_X4": {
                "Scope": "system/MedicationRequest.read system/*.read"
            }
        }
    },
    "OpenIdConnect": {
        "Audience": "pharmanet",
        "Authority": "{keycloak realm license plate URL}",
        "ClaimsIssuer": "{keycloak realm license plate URL}",
        "Scope": "system/MedicationRequest.write system/*.write system/MedicationRequest.read system/*.read"
    },
    "PharmanetProxy": {
        "Endpoint": "{pharmanet_endpoint}",
        "Password": "{password}",
        "Username": "{username}",
        "ClientCertificatePath": "/usr/local/pharmanet/d1pnetgwy.pfx",
        "ClientCertificatePassword": "{cert password}"
    }
}
